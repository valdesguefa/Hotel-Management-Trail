<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="450.0" prefWidth="500.0" spacing="20.0" styleClass="mover" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SignupController">
   <children>
      <Label styleClass="title" text="Create Hotel Manager Account" />
      <TextField fx:id="nameTF" promptText="Full name" />
      <TextField fx:id="emailTF" layoutX="10.0" layoutY="231.0" promptText="Email" />
      <TextField fx:id="passwordTF" layoutX="10.0" layoutY="244.0" promptText="Password" />
      <ComboBox fx:id="securityQuessCB" prefWidth="400.0" promptText="Choose security question" />
      <TextField fx:id="answerTF" promptText="Answer" />
      <Button fx:id="signUpBtn" mnemonicParsing="false" text="Sign Up" />
   </children>
   <padding>
      <Insets bottom="60.0" left="60.0" right="60.0" top="60.0" />
   </padding>
</VBox>
