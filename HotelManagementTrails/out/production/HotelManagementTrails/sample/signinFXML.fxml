<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="450.0" prefWidth="500.0" spacing="40.0" styleClass="mover" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SigninController">
   <children>
      <Label styleClass="title" text="Sign In to Hotel Manager" />
      <TextField promptText="Email" />
      <TextField layoutX="10.0" layoutY="290.0" promptText="Password" />
      <Button fx:id="loginBtn" layoutX="184.0" layoutY="315.0" mnemonicParsing="false" text="Sign In" />
      <Button mnemonicParsing="false" text="Forgotten Password?" />
   </children>
   <padding>
      <Insets bottom="60.0" left="60.0" right="60.0" top="60.0" />
   </padding>
</VBox>
